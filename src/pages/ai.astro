---
import Project from "../components/Project.astro";
import Layout from "../layouts/Layout.astro";
const skills = [];
---

<Layout
  title="Artificial Intelligence"
  description="Detailed information about Artificial Intelligence by Jonathan Albarran, skills and technology stack."
  image="/puertofolio/assets/preview.png"
  selectedItem="ai"
>
  <div class="content-container">
    <h1>Artificial Intelligence</h1>
    <section class="items-container">
      <div class="resouce-items">
        {
          skills.map((skill) => (
            <Project title={skill.title} desc={skill.desc} tech={skill.tech} />
          ))
        }
      </div>
    </section>
  </div>
</Layout>
